import java.util.Scanner;
public class Output
{
 public static void main(String args[])
 {
  int n,i,j,a;
  Scanner in=new Scanner(System.in);
  System.out.println("Enter the value of n:");
  n=in.nextInt();
  a=1;
  for(i=1;i<=n;i++)
  {
   for(j=0;j<i;j++)
   {
    System.out.print(a+"\t");
    a++;
   }
   System.out.println();
  }
 }
}
